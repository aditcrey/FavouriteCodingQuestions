#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	
	while(t--){
	    int n; 
	    cin>>n;
	    
	    int x = n&(-n); // taking and of the number and its negative
	    
	    int count = 0;
	    while(x!=0){
	        x=x>>1;
	        count++;
	    }
	    
	    cout<<count<<endl;
	    
	    
	    
	}
	return 0;
}
